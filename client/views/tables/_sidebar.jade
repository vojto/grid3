template(name='table_sidebar')
  h4.small Sources
  ul.sidebar-list
    each sources
      li.source(class=currentClass, data-id=_id)
        i.entypo.large.cloud
        span= title
        i.small.delete-source.entypo.circled-cross

  h4.small Processing

  ul.sidebar-list
    each steps
      li.step(class=currentClass, data-id=_id)
        i.large.entypo(class='cog')
        span= title
        i.small.delete-step.entypo.circled-cross

  h4.small Visualizations
