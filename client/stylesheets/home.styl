@import '_colors'
@import 'nib'

// Sources

ul.sources
  list-style none
  padding 0
  clearfix()
  text-align center
  margin-top 30px

  li
    width 80px
    float left
    margin-bottom 20px
    cursor pointer

  i.entypo.link
    font-size 48px

  small
    color fg2
    font-size 12px

    i.entypo
      cursor pointer
    i.entypo:hover
      color fg1